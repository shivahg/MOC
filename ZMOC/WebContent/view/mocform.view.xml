					editable="{= (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'C' )|| (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'S')}"
					width="60%" id="significant" 
				items="{
					 path:'values>/',
					 templateShareable:false
					 }" selectionChange="significant_change" selectedKey="{mocmodel>/empdetails/Significant}">
				<core:Item  text="{values>y}"  key="{values>y}"/>
					</ComboBox>
							
							
						
							
							
							
							
							<Label  design="Bold" required="true" text="Change Type"/>
							
						<ComboBox
							editable="{= (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'C' )|| (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'S')}"
