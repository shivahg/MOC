					 <Label design="Bold" text="From Date"  required="true"/>
					<DatePicker width="60%" id="datefrom" value="{path:'mocmodel>/empdetails/DateFrom',type:'sap.ui.model.type.DateTime',
								formatOptions:{
								pattern:'yyyy/MM/dd'
								}
								}" visible="{= (${mocmodel>/loginMode} === 'E' &amp;&amp;${mocmodel>/empdetails/KindChange} === 'Temporary' )||(${mocmodel>/loginMode} === 'A' &amp;&amp;${mocmodel>/empdetails/KindChange} === 'Temporary' )}" editable="{= (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'C' )|| (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'S')}"/> 
