								formatOptions:{
								pattern:'yyyy/MM/dd'
								}
								}" 
							enabled="{= (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'C' )|| (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'S')}"/> 
							<!-- <DatePicker width="60%" id="mocdate"  value="{mocmodel>/empdetails/MocDate}" 
							editable="true"/> -->
							
							
							<Label design="Bold" required="true" text="Kind of change"/>
