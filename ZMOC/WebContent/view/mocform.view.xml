					width="60%" id="kc"
				items="{
					 path:'values>/',
					 templateShareable:false
					 }" selectionChange="kind_change"  selectedKey="{mocmodel>/empdetails/KindChange}">
				<core:Item  text="{values>perm}" key="{values>perm}" />
					</ComboBox>
					<Label design="Bold" text="From Date"  required="true"/>
