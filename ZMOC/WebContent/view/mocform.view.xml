<mvc:View controllerName="ZCCM.ZCCM.controller.mocform" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
xmlns:f="sap.f" xmlns:layout="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Shell id="shell">
		<App>
			<pages>
				<Page id="page" title="MOC Form">
					<content>
						
					
			<f:DynamicPage id="dynamicPageId">
				<f:title>
					<f:DynamicPageTitle>
						<f:heading>
						<Title text="Management Of Change Creation"/>
					
					</f:heading>
					</f:DynamicPageTitle>
				</f:title>
				<f:header>
				<f:DynamicPageHeader pinnable="true">
					<layout:HorizontalLayout allowWrapping="true">
						<layout:VerticalLayout class="sapUiMediumMarginEnd">
							 <ObjectAttribute title="MOC Number" text="{mocmodel>/empdetails/MocNum}"/>
							<ObjectAttribute title="Status" text="{mocmodel>/empdetails/Status}"/> 
				           <!--  <ObjectAttribute title="Document No" text="12345"/> -->
							</layout:VerticalLayout>
                             </layout:HorizontalLayout>
					
			</f:DynamicPageHeader>
			</f:header>
				<f:content>
					<VBox>
					<form:SimpleForm editable="true" layout="ResponsiveGridLayout" title="MAKE OF CHANGE FORM">
					<Label  design="Bold" required="true" text="MOC Title"/>
