						    editable="{= (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'C' )|| (${mocmodel>/loginMode} === 'E' &amp;&amp; ${mocmodel>/Sav_sub} === 'S')}"  
						    showValueHelp="true" valueHelpRequest="plantunit" liveChange="onsuggest_plant" suggestionItems="{path:'plantdata>/'}" showSuggestion="true" growing="true">
						   <suggestionItems>
				<core:Item text="{plantdata>plant}" />
			</suggestionItems>
		</Input>
							
							<Label design="Bold" required="true" text="Significant (Yes/No)"/>
