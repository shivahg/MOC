					 path:'values>/',
					 templateShareable:false
					 }" selectionChange="kind_change" selectedKey="{mocmodel>/empdetails/Planned}">
				<core:Item  text="{values>y}" key="{values>y}" />
					</ComboBox>
					
							
						
		
				
