				items="{
					 path:'/empdetails',
					 sorter:{path:'type'}
					 }" selectionChange="onselect_changetype"  value="{mocmodel>/empdetails/ChangeType}">
				<core:Item  text="{type}" />
					</ComboBox>
                            <Label design="Bold" required="true" text="Employee Number"/>
