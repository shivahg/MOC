							liveChange="onsuggest_empno" suggestionItems="{path:'empdata>/'}" suggestionItemSelected="onselect_empno">
									   <suggestionItems>
				<core:Item key="{empdata>Vorna}" text="{empdata>Pernr}" />
			</suggestionItems>
		</Input>
							<Label design="Bold" required="true" text="Employee Name"/>
